    h3 Редактировать событие

  +jsVar('event', event)
  form.form-horizontal(role='form', method='POST', ng-controller='EditEventCtrl',
    ng-submit='submit()', name='eventForm', novalidate
  )
    +eventFields(false)
    +formBtns()